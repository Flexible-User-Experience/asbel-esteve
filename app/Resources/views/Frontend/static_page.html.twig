{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-xs-12">
            <h1>{{ page.title }}</h1>
            <p>{{ page.description | raw }}</p>
        </div>
    </div>
{% endblock %}

{% block bottom %}
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                {{ form(form) }}
            </div>
        </div>
    </div>
    {{ parent() }}
{% endblock %}
